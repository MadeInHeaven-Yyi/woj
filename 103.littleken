#include<stdio.h>

int main(void){
       int nCase,pIn;
       float p,ans;
       scanf("%d",&nCase);
       while(nCase--){
             scanf("%d",&pIn);
             getchar();//百分号
             p=(float)pIn/100;//百分数转成小数
             ans=(float)p*p/(1-2*p*(1-p))*100;
             printf("%.2f%c\n",ans,'%');
             }
        return 0;
}
